    "@typescript-eslint/parser": "6.13.1",
    "@types/aws-lambda": "8.10.129",
    "@types/jest": "29.5.10",
    "aws-sdk": "2.1509.0",
    "aws-sdk-mock": "5.8.0",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "15.1.0",
    "prettier": "3.1.0",
    "serverless": "3.38.0",
    "serverless-esbuild": "1.49.0",
    "serverless-offline": "13.3.0",
